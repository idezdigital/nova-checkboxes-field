{
    "name": "idez/nova-checkboxes-field",
    "description": "A Laravel Nova Checkboxes field.",
    "keywords": [
        "laravel",
        "nova",
        "idez",
        "idez digital",
        "custom field",
        "checkbox",
        "checkboxes"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Idez\\NovaCheckboxesField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Idez\\NovaCheckboxesField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
